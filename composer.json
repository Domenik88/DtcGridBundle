{
    "name": "domenik88/grid-bundle",
    "description": "Datagrid for symfony4 or symfony5 with DoctrineORM after 3.0",
    "keywords": ["grid", "jQGrid", "table"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "David Tee"
        },
        {
            "name": "Matthew J Mucklo",
            "email": "mmucklo@gmail.com"
        },
        {
            "name": "Eugene K",
            "email": "kataevevgenii@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "symfony/framework-bundle": "4.*|5.*",
        "sensio/framework-extra-bundle": "4.*|5.*|6.*",
        "symfony/templating": "4.*|5.*",
        "symfony/translation": "4.*|5.*",
        "symfony/twig-bundle": "4.*|5.*",
        "doctrine/doctrine-bundle": "^2.2",
        "twig/twig": "^1.0||^2.0|^3.0"
    },
    "require-dev": {
        "doctrine/orm": "3.0.x-dev",
        "doctrine/annotations": "^1.12",
        "doctrine/cache": "^1.9",
        "doctrine/collections": "^1.5",
        "doctrine/instantiator": "^1.1",
        "doctrine/common": "^3.0",
        "doctrine/dbal": "^2.13",
        "phpunit/phpunit": "^5.7.0",
        "friendsofphp/php-cs-fixer": "^2.0",
        "phpunit/php-code-coverage": "^4.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "Dtc\\GridBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    }
}
